Execute (Get Current Mark, None Selected):
  normal! gg
  AssertEqual '', markbar#ui#GetCurrentMarkHeading()

Execute (Get Next Mark):
  normal! gg
  AssertEqual 2, markbar#ui#GetNextMarkHeadingLine()

Execute (Get Current Mark, Cursor Over Section Header):
  normal! 2G
  AssertEqual 2,   markbar#ui#GetCurrentMarkHeadingLine()
  AssertEqual 'A', markbar#ui#GetCurrentMarkHeading()

Execute (Get Next Mark, From Section Header):
  normal! 2G
  AssertEqual 8, markbar#ui#GetNextMarkHeadingLine()

Execute (Get Current Mark, From Mark Context):
  normal! 10G
  AssertEqual 8,  markbar#ui#GetCurrentMarkHeadingLine()
  AssertEqual 'B', markbar#ui#GetCurrentMarkHeading()

Execute (Get Next Mark, From Mark Context):
  normal! 10G
  AssertEqual 14, markbar#ui#GetNextMarkHeadingLine()

Execute (Get Previous Mark, From Bottom of File):
  normal! G
  AssertEqual 26, markbar#ui#GetPreviousMarkHeadingLine()

Execute (Get Previous Mark, From Mark Context):
  normal! 21G
  AssertEqual 20, markbar#ui#GetPreviousMarkHeadingLine()

Execute (Get Previous Mark, From Section Header):
  normal! 14G
  AssertEqual 8, markbar#ui#GetPreviousMarkHeadingLine()
